#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){
    int n,*a,x,y,j,i,count=0;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        count=0;
        scanf("%d%d",&x,&y);
        a=malloc(x * sizeof(int));
        for(j=0;j<x;j++){
            scanf("%d",&a[j]);
           if(a[j]<=0)
            count++;
        }
       if(count>=y)
      // printf("%d ",count);
       printf("NO\n");
       else 
    printf("YES\n");
    }
    return 0;
}
